{
  "extends": ["eslint:recommended", "plugin:jest/recommended"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "jest"],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
      "semi": "error"
  },
  "env": {
    "browser": true,
    "jest/globals": true
  }
}
