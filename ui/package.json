  "scripts": {
    "clean": "rm public/bundle.*",
    "build": "rollup -c --environment DEV:false",
    "build-dev": "rollup -c --environment DEV:true",
    "autobuild": "rollup -c -w --environment DEV:true",
    "autobuild-playground": "rollup -c -w --environment DEV:true,PLAYGROUND:true",
    "dev": "run-p start:dev autobuild",
    "playground": "run-p start:dev autobuild-playground",
    "start": "sirv public",
    "start:dev": "sirv public --dev",
    "lint": "eslint --ext ts src/js test",
    "jest": "jest",
    "jest-ignore-remote": "jest --testPathIgnorePatterns RemoteRepository\\.test\\.ts",
    "test": "run-s lint jest-ignore-remote",
    "test-remote": "jest --testPathPattern RemoteRepository\\.test\\.ts",
    "atest": "jest -i",
    "build:tailwind": "tailwind build tailwind.css -o public/tailwind-gen.css"
  },
  "dependencies": {}
